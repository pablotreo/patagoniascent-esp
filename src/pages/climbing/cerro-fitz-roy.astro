  {
    alt: 'Cerro Fitz Roy',
    srcFotos: '../img/paginas/slider/climbing/cerrofitzroy/fitzroy1.jpg',
    srcThum: '../img/paginas/slider/climbing/cerrofitzroy/fitzroythumb1.jpg',
  },
  {
    alt: 'Cerro Fitz Roy',
    srcFotos: '../img/paginas/slider/climbing/cerrofitzroy/fitzroy2.jpg',
