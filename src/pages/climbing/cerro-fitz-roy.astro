  },
  {
    alt: 'Cerro Fitz Roy',
    srcFotos: '../img/paginas/slider/climbing/cerrofitzroy/fitzroy6.jpg',
    srcThum: '../img/paginas/slider/climbing/cerrofitzroy/fitzroythumb6.jpg',
  },
  {
    alt: 'Cerro Fitz Roy',
    srcFotos: '../img/paginas/slider/climbing/cerrofitzroy/fitzroy7.jpg',
